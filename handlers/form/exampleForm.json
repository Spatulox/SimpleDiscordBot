{
  "title": "Complete Form",
  "id": "complete_form",
  "inputs": [
    {
      "type": "text",
      "id": "simple_text",
      "title": "Simple Text",
      "style": "short",
      "required": true
    },
    {
      "type": "text",
      "id": "long_text",
      "title": "Long Text",
      "style": "long",
      "required": false
    },
    {
      "type": "text_placeholder",
      "id": "text_with_placeholder",
      "title": "Text with Placeholder",
      "style": "short",
      "required": true,
      "placeholder": "Enter place holder here"
    },
    {
      "type": "text_minmax_length",
      "id": "text_with_length",
      "title": "Text with Min/Max size",
      "style": "long",
      "required": true,
      "minLength": 10,
      "maxLength": 100
    },
    {
      "type": "number",
      "id": "number_input",
      "title": "Numeral entry",
      "required": true
    },
    {
      "type": "date",
      "id": "date_input",
      "title": "Date",
      "required": true
    }
  ]
}
